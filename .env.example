# Copy this file to .env and fill in your values.

# Google Gemini — single key used by scorer.py AND bot/telegram_bot.py
GOOGLE_API_KEY=

# Telegram bot token — obtained from @BotFather on Telegram.
TELEGRAM_BOT_TOKEN=

# Telegram supergroup chat_id — negative integer (e.g. -1001234567890).
# Used by run_biweekly.py to post digest announcements.
# Find it by adding @userinfobot to your group, or from the URL in Telegram Web.
TELEGRAM_CHAT_ID=
